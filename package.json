{
  "name": "idb-lru-cache",
  "description": "IndexedDB-based persisting LRU cache for the browser",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://make-github-pseudonymous-again.github.io/idb-lru-cache",
  "repository": {
    "url": "https://github.com/make-github-pseudonymous-again/idb-lru-cache",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/idb-lru-cache/issues"
  },
  "keywords": [
    "browser",
    "cache",
    "database",
    "db",
    "idb",
    "indexed",
    "least",
    "lru",
    "recently",
    "used"
  ],
  "sideEffects": false,
  "type": "module",
  "source": "src/mod.ts",
  "types": "dist/mod.d.ts",
  "main": "dist/mod.cjs",
  "module": "dist/mod.mjs",
  "exports": {
    "require": "./dist/mod.cjs",
    "import": "./dist/mod.mjs",
    "default": "./dist/mod.modern.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "NODE_ENV=production microbundle -f modern,esm,cjs  && cp -l dist/src/mod.d.ts dist/mod.d.ts",
    "build-docs": "typedoc src/mod.ts",
    "build-gh-pages": "yarn build-docs --out gh-pages",
    "commit-msg": "commitlint --edit",
    "cover": "NODE_ENV=cover c8 --all --src src --reporter lcov --reporter text-summary --reporter text yarn test",
    "install-hooks": "husky install",
    "lint": "xo",
    "lint-and-fix": "xo --fix",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "yarn install-hooks",
    "postpack": "pinst --enable",
    "precommit": "lint-staged",
    "prepack": "pinst --disable",
    "prepare": "yarn build",
    "release": "np --message ':hatching_chick: release: Bumping to v%s.'",
    "tc": "tsc --noEmit",
    "tc:watch": "yarn tc --noErrorTruncation --watch",
    "test": "aba test --timeout 30s"
  },
  "dependencies": {
    "idb": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@js-library/commitlint-config": "0.0.4",
    "@types/chai": "4.3.4",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.13.0",
    "chai": "4.3.7",
    "fixpack": "4.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "microbundle": "0.15.1",
    "np": "7.6.3",
    "pinst": "3.0.0",
    "power-assert": "1.6.1",
    "seedrandom": "3.0.5",
    "tap-dot": "2.0.0",
    "tslib": "2.5.0",
    "typedoc": "0.23.26",
    "typedoc-plugin-coverage": "2.0.0",
    "typescript": "4.8.4",
    "xn--mxaac": "0.1.1",
    "xo": "0.53.1"
  },
  "babel": {
    "sourceMaps": true,
    "plugins": [
      [
        "transform-remove-console",
        {
          "exclude": [
            "log",
            "error",
            "warn"
          ]
        }
      ]
    ],
    "env": {
      "debug": {
        "sourceMaps": "both",
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "debug",
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "test": {
        "sourceMaps": "both",
        "presets": [
          "babel-preset-power-assert"
        ]
      },
      "cover": {
        "sourceMaps": "both",
        "presets": [
          "babel-preset-power-assert"
        ]
      },
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": [
                "defaults",
                "maintained node versions"
              ]
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": [
                "defaults",
                "maintained node versions"
              ]
            }
          ]
        ],
        "plugins": [
          "babel-plugin-unassert"
        ]
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint-and-fix"
    ],
    "package.json": [
      "yarn lint-config-and-fix"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  },
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "env": "browser",
    "rules": {
      "import/extensions": [
        "error",
        "never"
      ],
      "unicorn/filename-case": "off",
      "unicorn/prefer-node-protocol": "off",
      "no-bitwise": "off",
      "@typescript-eslint/ban-types": "off"
    },
    "overrides": [
      {
        "files": [
          "src/**"
        ],
        "rules": {
          "@typescript-eslint/naming-convention": "off",
          "unicorn/error-message": "off",
          "unicorn/prefer-math-trunc": "off"
        }
      },
      {
        "files": [
          "test/**"
        ],
        "rules": {
          "no-await-in-loop": "off"
        }
      }
    ]
  }
}
